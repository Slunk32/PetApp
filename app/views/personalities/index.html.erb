<p id="notice"><%= notice %></p>

<h1>Listing Personalities</h1>

<table>
  <thead>
    <tr>
      <th>Trait</th>
      <th>Pet</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @personalities.each do |personality| %>
      <tr>
        <td><%= personality.trait %></td>
        <td><%= personality.pet.name %></td>
        <td><%= link_to 'Show', personality %></td>
        <td><%= link_to 'Edit', edit_personality_path(personality) %></td>
        <td><%= link_to 'Destroy', personality, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

