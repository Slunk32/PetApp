<h2 id="headingfont">Your Profile</h2>
<!-- This is our show profile page -->
<div class="signup-wrap">
  <div class="field">
    <p id="notice"><%= notice %></p>
  <p>
    <strong>Type:</strong>
    <%= @user.user_type %>
  </br>
  </p>

  <p>
    <strong>Email:</strong>
    <%= @user.email %>
  </p>
  <p>
    <%= image_tag(current_user.avatar.url(:medium)) %>
  </p>

  <div class="edit_btn">
    <%= link_to 'Edit', edit_user_registration_path %>
  </div>
</div>
</div>
